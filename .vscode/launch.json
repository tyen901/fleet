{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Fleet UI (fleet)",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "run",
                    "--package",
                    "fleet",
                    "--bin",
                    "fleet"
                ],
                "filter": {
                    "name": "fleet",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}",
            "env": {
                "RUST_BACKTRACE": "1"
            }
        }
    ]
}
